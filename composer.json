{
    "name": "thebigcrafter/advancedpluginmanager",
    "description": "A plugin manager for PocketMine servers",
    "type": "project",
    "require": {
        "pocketmine/pocketmine-mp": "^3.25",
        "cortexpe/commando": "@dev",
        "jojoe77777/formapi": "@dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2"
    },
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Thebigcrafter\\Advancedpluginmanager\\": "src/thebigcrafter/APM/"
        }
    },
    "scripts": {
        "php-cs-fixer": "./vendor/bin/php-cs-fixer"
    },
    "authors": [
        {
            "name": "MintoD",
            "email": "tobythedev7@outlook.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./lib/Commando-master/"
        },
        {
            "type": "path",
            "url": "./lib/FormAPI-master/"
        }
    ]
