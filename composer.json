{
    "name": "thebigcrafter/advancedpluginmanager",
    "description": "Plugin manager for PocketMine-MP",
    "type": "project",
    "require": {
        "php": "^8.0",
        "pocketmine/pocketmine-mp": "^4.0",
        "cortexpe/commando": "dev-PM4",
        "dktapps/pmforms": "dev-master"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "friendsofphp/php-cs-fixer": "^3.3"
    },
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "thebigcrafter\\APM\\": "src/"
        }
    },
    "scripts": {
        "fix-cs": "./vendor/bin/php-cs-fixer fix src/",
        "analyse": "./vendor/bin/phpstan analyse -c phpstan.neon"
    },
    "authors": [
        {
            "name": "MintoD",
            "email": "tobythedev7@outlook.com",
            "homepage": "https://github.com/MintoD",
            "role": "Developer"
        },
        {
            "name": "Mcbeany",
            "homepage": "https://github.com/Mcbeany",
            "role": "Contributor"
        },
        {
            "name": "NhanAZ",
            "email": "nhanaz@pm.me",
            "homepage": "https://github.com/NhanAZ",
            "role": "Contributor"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/MintoD/pmforms"
        }
    ]
}
