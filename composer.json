{
    "name": "thebigcrafter/advancedpluginmanager",
    "description": "Plugin manager for PocketMine-MP",
    "type": "project",
    "require": {
        "php": "^8.0",
        "pocketmine/pocketmine-mp": "^4.0",
        "cortexpe/commando": "dev-master",
        "dktapps/pmforms": "dev-master"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "friendsofphp/php-cs-fixer": "^3.3"
    },
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Thebigcrafter\\Advancedpluginmanager\\": "src/thebigcrafter/APM/"
        }
    },
    "scripts": {
        "fix-src": "./vendor/bin/php-cs-fixer fix src/",
        "analyse-src": "./vendor/bin/phpstan analyse -c phpstan.neon"
    },
    "authors": [
        {
            "name": "MintoD",
            "email": "tobythedev7@outlook.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/MintoD/pmforms"
        }
    ]
}
