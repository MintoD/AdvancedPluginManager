{
    "name": "thebigcrafter/advancedpluginmanager",
    "description": "A plugin manager for PocketMine servers",
    "type": "project",
    "require": {
        "php": "^8.0",
        "pocketmine/pocketmine-mp": "^3.25",
        "cortexpe/commando": "dev-master",
        "jojoe77777/libformapi": "@dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "phpstan/phpstan": "^1.1"
    },
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Thebigcrafter\\Advancedpluginmanager\\": "src/thebigcrafter/APM/"
        }
    },
    "scripts": {
        "fix-src": "./vendor/bin/php-cs-fixer fix src/",
        "analyse-src": "./vendor/bin/phpstan analyse -l 8 src/"
    },
    "authors": [
        {
            "name": "MintoD",
            "email": "tobythedev7@outlook.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/PocketMine-MP-VN-Group/FormAPI"
        }
    ]
}
